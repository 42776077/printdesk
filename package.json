{
  "name": "printdesk",
  "version": "1.0.14",
  "productName": "Printdesk",
  "description": "An electron app for devices to ServicePOS",
  "main": "main.js",
  "scripts": {
    "publish": "electron-builder --win -p always",
    "dev": "cross-env NODE_ENV=development electron .",
    "pack": "electron-builder --win --macos --dir",
    "build": "electron-builder --win"
  },
  "repository": "https://github.com/servicepos/printdesk",
  "build": {
    "appId": "com.servicepos.printdesk",
    "asarUnpack": [
      "assets/SumatraPDF.exe"
    ],
    "mac": {
    },
    "win": {
      "icon": "assets/servicepos.ico",
      "target": [
        "squirrel"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "servicepos",
        "repo": "printdesk"
      }
    ]
  },
  "postinstall": "electron-builder install-app-deps",
  "author": "ServicePOS",
  "license": "CC0-1.0",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "electron": "^5.0.1",
    "electron-builder": "^20.43.0",
    "electron-builder-squirrel-windows": "^20.43.0",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-installer-zip": "^0.1.2",
    "electron-reloader": "^0.3.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "aws-iot-device-sdk": "^2.2.1",
    "cmd-promise": "^1.2.0",
    "electron-log": "^3.0.5",
    "electron-prompt": "^1.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^3.2.0",
    "node-machine-id": "^1.1.10",
    "request": "^2.88.0",
    "tmp": "^0.1.0",
    "update-electron-app": "^1.3.0"
  }
}
